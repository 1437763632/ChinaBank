
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AddWeekly</title>
    <script src="~/Scripts/axios.js"></script>
    <script src="~/Scripts/vue.js"></script>
</head>
<body>
    <div id="add">
        <table class="table table-hover">
            <tr>
                <td>项目名称: </td>
                <td><input type="text" v-model="Formfirst" /></td>
            </tr>
            <tr>
                <td>项目名称: </td>
                <td><input type="text" v-model="Projectname" /></td>
            </tr>
            <tr>
                <td>项目经理:</td>
                <td><input type="text" v-model="Projmanager" /></td>
            </tr>
            <tr>
                <td>创建日期: </td>
                <td><input type="text" v-model="Creadate" /></td>
            </tr>
            <tr>
                <td>所属批次:</td>
                <td><input type="text" v-model="Batchone" /></td>
            </tr>
            <tr>
                <td>所属批次（同上）:</td>
                <td><input type="text" v-model="Batchtong" /></td>
            </tr>
            <tr>
                <td>所属部门: </td>
                <td><input type="text" v-model="Affiliated" /></td>
            </tr>
            <tr>
                <td>创建人:</td>
                <td><input type="text" v-model="Single" /></td>
            </tr>
            <tr>
                <td>本周时段: </td>
                <td><input type="text" v-model="Weekproj" /></td>
            </tr>
            <tr>
                <td>当前阶段: </td>
                <td><input type="text" v-model="Currentgeneration" /></td>
            </tr>
            <tr>
                <td>项目进度状态: </td>
                <td><input type="text" v-model="Projectstatus" /></td>
            </tr>
            <tr>
                <td>下周计划: </td>
                <td><input type="text" v-model="Nextweekproj" /></td>
            </tr>
            <tr>
                <td>备注: </td>
                <td><input type="text" v-model="Remark" /></td>
            </tr>
            <tr>
                <td>通知对象: </td>
                <td><input type="text" v-model="Notifyobj" /></td>
            </tr>
            <tr>
                <td><a href="javascript:;" v-on:click="AddQCPlan1(Formfirst,Projectname,Projmanager,Creadate,Batchone,Batchtong,Affiliated,Single,Weekproj,Currentgeneration,Projectstatus,Nextweekproj,Remark,Notifyobj)">添加</a></td>
                <td><a href="#" onclick="location.href='Index'">显示</a></td>
            </tr>
        </table>
    </div>
</body>
</html>
<script type="text/javascript">
    var vm = new Vue({
        el: "#add",
        data: {
            //Target: [],
            RoleName: "",
            RoleDescribe: ""
        },
        mounted() {
            //this.AddQCPlan1();
        },
        methods:
        {
            //保存
            AddQCPlan1: function (Formfirst, Projectname, Projmanager, Creadate, Batchone, Batchtong, Affiliated, Single, Weekproj, Currentgeneration, Projectstatus, Nextweekproj, Remark, Notifyobj) {
                var params = new URLSearchParams();
                params.append('Formfirst', Formfirst);
                params.append('Projectname', Projectname);
                params.append('Projmanager', Projmanager);
                params.append('Creadate', Creadate);
                params.append('Batchone', Batchone);
                params.append('Batchtong', Batchtong);
                params.append('Affiliated', Affiliated);
                params.append('Single', Single);
                params.append('Weekproj', Weekproj);
                params.append('Currentgeneration', Currentgeneration);
                params.append('Projectstatus', Projectstatus);
                params.append('Nextweekproj', Nextweekproj);
                params.append('Remark', Remark);
                params.append('Notifyobj', Notifyobj);
                axios.post('http://localhost:8086/Weekly/Add', params).then(
                    (response) => {
                        alert(response)
                        alert("添加成功")
                    },
                    (response) => {
                        alert(response.status);
                    }
                ).catch(function (response) {
                    alert(response);
                });
            },
        },
        computed: {
        }
    });
</script>
